interface
    id_interface 'i'
    # Use a Dakota node in an outer workflow to configure the specific .iwf file to use as the driver.
    analysis_drivers 'SAW_DRIVER' 
        fork
            parameters_file 'params.in'
            results_file 'results.out'
            file_tag
            file_save
            work_directory
                named 'dworkdir'
                directory_tag
                directory_save
    asynchronous
        evaluation_concurrency 3
        
variables
    id_variables 'v'

method
    id_method 'm'
 
 responses
    id_responses 'r'

environment
 	id_environment 'e'
 