   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="com.strikewire.snl.apc.Common.MessageSplashHandler"
            id="gov.sandia.dart.workflow.app.splashHandler">
      </splashHandler>
      <splashHandlerProductBinding
            productId="gov.sandia.dart.workflow.app.Sandia_Analysis_Workbench"
            splashId="gov.sandia.dart.workflow.app.splashHandler">
      </splashHandlerProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.statusHandlers">
       <statusHandlerProductBinding
            handlerId="com.strikewire.snl.apc.statusHandler"
            productId="gov.sandia.dart.workflow.app.Sandia_Analysis_Workbench">
      </statusHandlerProductBinding>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.eclipse.core.runtime.text"
            file-extensions="ini,sh,log">
      </file-association>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.internalTweaklets">
      <tweaklet
            definition="org.eclipse.ui.internal.tweaklets.GrabFocus"
            description="Tweak to prevent the problems view from grabbing focus and ultimately un-zooming (putting in stand-by) the welcome page"
            id="gov.sandia.dart.workflow.app.tweaklet.grabFocus"
            name="Problems View No Focus"
            implementation="gov.sandia.dart.workflow.app.application.GrabFocusTweaklet"
            >
      </tweaklet>
   </extension>

