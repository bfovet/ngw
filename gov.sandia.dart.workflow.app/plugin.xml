<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
      <extension
            point="org.eclipse.ui.actionSets">
         <actionSet
               description="Workbench SIMBA"
               id="gov.sandia.dart.simba.plugins.core.modeleditor"
               label="Workbench Model Manager"
               visible="true">
            <action
                  class="gov.sandia.dart.simba.plugins.core.ModelManagerActionDelegate"
                  icon="icons/simba.gif"
                  id="gov.sandia.dart.simba.launch"
                  label="Workbench Model Manager"
                  menubarPath="tools/additions"
                  style="push"
                  toolbarPath="org.eclipse.ui.workbench.file/build.ext"
                  tooltip="Launch the Workbench Model Manager">
            </action>
         </actionSet>
      </extension>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            icon="icons/shapes.gif"
            class="gov.sandia.dart.workflow.app.ApcWorkbench.rcp.WorkflowPerspective"
            name="Workflow"
            id="gov.sandia.dart.workflow.app.perspective.workflow">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="gov.sandia.dart.workflow.app.perspective.workflow">
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.ui.resourcePerspective">
         <perspectiveShortcut id="gov.sandia.dart.workflow.app.perspective.workflow"/>
         <viewShortcut id="gov.sandia.apc.views.ApcResourceNavigator"/>
         <perspectiveShortcut id="gov.sandia.dart.workflow.app.DfmPerspective"/>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="gov.sandia.dart.workflow.app.perspective.job">            
         <view
               id="org.eclipse.ui.console.ConsoleView"
               minimized="false"
               ratio="0.6f"
               relationship="bottom"
               relative="org.eclipse.ui.editorss">
         </view>
         <view
               id="com.strikewire.snl.apc.FileManager.views.MachineView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.console.ConsoleView">
         </view>
         <view
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               ratio="0.6f"
               relationship="bottom"
               relative="com.strikewire.snl.apc.projectexplorer.views.ProjectExplorerView">
         </view>
         <view
               id="com.strikewire.snl.apc.ProjectExplorer.DependencyTreeView"
               minimized="false"
               relationship="stack"
               relative="com.strikewire.snl.apc.FileManager.views.MachineView">
         </view>
         <view
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               relationship="stack"
               relative="com.strikewire.snl.apc.FileManager.views.MachineView">
         </view>
         <view
               id="org.eclipse.ui.views.ProgressView"
               minimized="false"
               relationship="stack"
               relative="com.strikewire.snl.apc.FileManager.views.MachineView">
         </view>
         <view
               id="gov.sandia.apc.JobSubmission.JobStatusView"
               minimized="false"
               ratio="0.7f"
               relationship="right"
               relative="org.eclipse.ui.editorss">
         </view>
         <view
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               ratio="0.7f"
               relationship="stack"
               relative="gov.sandia.apc.JobSubmission.JobStatusView">
         </view>
         <view
               id="com.strikewire.snl.apc.FileManager.views.FileView"
               minimized="false"
               ratio="0.7f"
               relationship="stack"
               relative="gov.sandia.apc.JobSubmission.JobStatusView">
         </view>
         <newWizardShortcut
               id="gov.sandia.dart.materials.ui.wizards.NewMDF">
         </newWizardShortcut>
         <newWizardShortcut
               id="gov.sandia.apc.wizards.NewInputDeckWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="gov.sandia.dart.jaguar.ProcessWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.folder">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.file">
         </newWizardShortcut>                      
      </perspectiveExtension>
   </extension>
   <extension
         id="Sandia_Analysis_Workbench"
         name="Sandia Analysis Workbench"
         point="org.eclipse.core.runtime.products">
      <product
            application="gov.sandia.dart.workflow.app.Workbench_Application"
            description="Sandia Analysis Workbench Client"
            name="Sandia Analysis Workbench">
         <property
               name="windowImages"
               value="icons/app/16x16.gif,icons/app/32x32.gif,icons/app/48x48.gif">
         </property>
         <property
               name="aboutText"
               value="%aboutText">
         </property>
         <property
               name="aboutImage"
               value="icons/app/48x48.gif">
         </property>
         <property
               name="startupForegroundColor"
               value="FFFFFF">
         </property>
         <property
               name="startupProgressRect"
               value="0,274,431,10">
         </property>
         <property
               name="startupMessageRect"
               value="7,2,424,20">
         </property>
         <property
               name="appName"
               value="Sandia Analysis Workbench">
         </property>
         <property
               name="windowImage"
               value="icons/sandia32.gif">
         </property>
         <property
               name="appVersion"
               value="{0}">
         </property>
         <property
               name="splashMessage"
               value="%splashText">
         </property>
         <property
               name="splashMessagePoint"
               value="-7,215">
         </property>
         <property
               name="cssTheme"
               value="gov.sandia.dart.ui.themes.theme.saw">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         
      </product>
   </extension>
   <extension
         id="Workbench_Application"
         name="Sandia Analysis Workbench Application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="gov.sandia.dart.workflow.app.application.DartWorkbenchApplication">
         </run>
      </application>
   </extension>
